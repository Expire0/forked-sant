    <addon id="repository.origin" name="Origin Repo" version="1.0.1" provider-name="Origin">
        <requires>
            <import addon="xbmc.addon" version="12.0.0"/>
        </requires>
        <extension point="xbmc.addon.repository" name="Origin Repo">
            <info compressed="false">https://raw.github.com/toall1985/origin/master/_repo/addons.xml</info>
            <checksum>https://raw.github.com/toall1985/origin/master/_repo/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.github.com/toall1985/origin/master/_repo/</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>A short summary of this addon</summary>
            <description>Origin Builds Repo</description>
            <platform>all</platform>
    </extension>
</addon>
</addons>
